{
  "name": "model-initializer",
  "version": "1.0.0",
  "description": "Create and validate database schemas with fully typesafe arrays.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && rollup -c rollup.config.mjs",
    "test": "node --experimental-specifier-resolution=node --loader ts-node/esm test/index.ts"
  },
  "exports": {
    "require": "./dist/cjs/index.cjs",
    "import": "./dist/esm/index.js",
    "default": "./dist/cjs/index.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanpmaxwell/model-initializer.git"
  },
  "keywords": [
    "model",
    "record",
    "node",
    "schema",
    "postgres",
    "validate",
    "validator",
    "inti",
    "initialzer",
    "create",
    "validation"
  ],
  "author": "sean maxwell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seanpmaxwell/model-initializer/issues"
  },
  "homepage": "https://github.com/seanpmaxwell/model-initializer#readme",
  "dependencies": {
    "lodash": "^4.17.21",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "rimraf": "^6.0.1",
    "rollup": "^4.19.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  }
}
